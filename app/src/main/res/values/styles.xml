<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Widget.SeekBar.Normal" parent="@android:style/Widget.SeekBar">

        <item name="android:indeterminateOnly">false</item>

        <item name="android:progressDrawable">@drawable/bg_music_player_seek_bar</item>

        <item name="android:thumb">@drawable/drager_seek_bar</item>
        <item name="android:thumbOffset">0dip</item>

    </style>
</resources>
